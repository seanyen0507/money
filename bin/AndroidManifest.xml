<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moneymoney"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
		<activity android:name="com.facebook.LoginActivity"/>
        
        <activity
            android:name="com.example.moneymoney.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.moneymoney.RentPage"
            android:label="@string/title_activity_rent_page" >
        </activity>
        <activity
            android:name="com.example.moneymoney.Friend"
            android:label="@string/title_activity_friend" >
        </activity>
        <activity
            android:name="com.example.moneymoney.Facebook"
            android:label="@string/title_activity_facebook" >
        </activity>
        <activity
            android:name="com.example.moneymoney.Setting"
            android:label="@string/title_activity_setting" >
        </activity>
        <meta-data android:value="@string/facebook_app_id" android:name="com.facebook.sdk.ApplicationId"/>
    </application>

</manifest>
